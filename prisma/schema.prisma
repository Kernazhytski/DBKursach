model City {
  id Int @id @default(autoincrement())

  name String

  country    Country      @relation(fields: [country_id], references: [id])
  country_id Int

  Labaratory Labaratory[]
}
model Country {
  id Int @id @default(autoincrement())

  name String

  City City[]
}
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Labaratory {
  id Int @id @default(autoincrement())

  constructedYear   Int
  constructedMounth Int
  constructedDay    Int

  city    City @relation(references: [id], fields: [city_id])
  city_id Int

  name String

  User User[]
}
model Metrics {
  id Int @id @default(autoincrement())

  type       MetricsType
  value      Float
  created_at DateTime    @default(now())

  User    User   @relation(fields: [user_id], references: [id])
  user_id String
}
model Quiz {
  id Int @id @default(autoincrement())

  user    User   @relation(fields: [user_id], references: [id])
  user_id String

  question    QuizQuestions
  answer      Int
  answer_date DateTime      @default(now())
}
model TestResults {
  id String @id

  user    User   @relation(fields: [user_id], references: [id])
  user_id String

  type  TestType
  value Float
  unit  TestUnits

  TestYear   Int
  TestMounth Int
  TestDay    Int
}
model User {
  id     String  @id
  name   String
  gender Gender
  phone  String?
  email  String?
  year   Int
  mounth Int
  day    Int

  labaratory    Labaratory @relation(fields: [labaratory_id], references: [id])
  labaratory_id Int

  Metrics   Metrics[]
  Quiz      Quiz[]
  TestResults TestResults[]
}
enum Gender {
  MALE
  FEMALE
}
